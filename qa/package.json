{
  "name": "qa",
  "version": "0.1.0",
  "description": "End to End tests for Moonlight",
  "main": "index.ts",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "4.1.1",
    "@types/cypress-cucumber-preprocessor": "1.14.0",
    "@types/node": "13.1.4",
    "cypress": "3.8.1",
    "cypress-cucumber-preprocessor": "1.18.0",
    "ts-loader": "6.2.1",
    "webpack": "4.41.5"
  },
  "scripts": {
    "pretest": "npm install",
    "test": "cypress run --record --spec **/*.feature",
